{
  "name": "ecosystem",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "graph": "turbo run build --graph=graph.png"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/styled-components": "^5.1.26",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "styled-components": "^5.3.6"
  },
  "packageManager": "pnpm@7.4.1"
}